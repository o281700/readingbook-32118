<div class="screen-field">
  <div class='side-wrapper'>
    <%= render partial: 'books/side_ber' %>
  </div>
  <div class='main-wrapper'>
    <p class='nickname-user-info'><%= @nickname %>の投稿一覧</p>
    <% @books.each do |book| %>
      <div class='user-show'>
        <div class='book-check'>
          <%= check_box_tag "books_ids[]", book.id %>
        </div>
        <div>
          <h2><%= book.title %></h1>
          <h4><%= book.author %></h3>
          <p><%= book.impression %></p>
        </div>
      </div>
    <% end %>
  </div>
</div>